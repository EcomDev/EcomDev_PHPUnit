{
    "name": "ecomdev/ecomdev_phpunit",
    "license": "OSL-3.0",
    "type": "magento-module",
    "description": "Magento PHPUnit Integration",
    "homepage": "http://www.ecomdev.org/shop/code-testing/php-unit-test-suite.html",
    "require": {
        "magento-hackathon/magento-composer-installer": "*",
        "phpunit/phpunit": "^9.5"
    },
    "replace": {
        "ivanchepurnyi/ecomdev_phpunit":"*"
    },
    "authors":[
        {
            "name":"Ivan Chepurnyi"
        }
    ],
    "config": {
        "allow-plugins": {
            "magento-hackathon/magento-composer-installer": true
        }
    },
    "extra": {
        "magento-root-dir": "root"
    }
}
